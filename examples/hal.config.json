{
  "globals": {
    "access": {
      "allowedUserIds": []
    },
    "engine": {
      "name": "claude"
    },
    "logging": {
      "level": "info",
      "flow": true,
      "persist": true
    },
    "rateLimit": {
      "max": 10,
      "windowMs": 60000
    },
    "transcription": {
      "model": "medium",
      "showTranscription": true
    }
  },
  "context": {
    "custom": "This is a custom context that will be included in every message sent to the LLM. You can use this to provide additional information or instructions to the LLM."
  },
  "projects": [
    {
      "name": "obsidian",
      "cwd": "./obsidian",
      "active": true,
      "engine": {
        "name": "copilot",
        "model": "claude-haiku-4.5"
      },
      "telegram": {
        "botToken": "${OBSIDIAN_TELEGRAM_TOKEN}"
      },
      "context": {
        "secondCustom": "This is a second custom context that will only be included in messages sent to the obsidian project."
      }
    },
    {
      "name": "timetracker",
      "cwd": "./timetracker",
      "telegram": {
        "botToken": "${TIMETRACKER_TELEGRAM_TOKEN}"
      }
    }
  ]
}
